<!DOCTYPE html>

<html lang='en'>
<head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
  <title>ejs</title>
  <style>
    h1 {
      text-align: center;
    }
    table {
      font-family: arial, sans-serif;
      border-collapse: collapse;
      width: 100%;
    }
    
    td, th {
      border: 1px solid #dddddd;
      text-align: left;
      padding: 8px;
    }
    
    tr:nth-child(even) {
      background-color: #dddddd;
    }
    </style>
</head>

<body>
  <h1>
    Test ejs
  </h1>
  <table>
    <tr>
      <th>name</th>
      <th>age</th>
      <th>birth_year</th>
    </tr>
  
    <% array.forEach(function(item) { %>
      <tr>
          <td><%= item.name %></td>
          <td><%= item.age %></td>
          <td><%= item.birth_year %></td>
         
      </tr>
  <% }); %>
    
   
  </table>

</body>
</html>